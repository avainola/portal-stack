{
  "compilerOptions":{
    "outDir":"lib",
    "module":"esnext",
    "target":"es5",
    "lib":["es5","dom"],
    "types":[],
    "allowJs":true,
    "jsx":"react",
    "moduleResolution":"node",
    "declaration":true,
    "forceConsistentCasingInFileNames":true,
    "noImplicitReturns":true,
    "noImplicitThis":true,
    "noImplicitAny":true,
    "strictNullChecks":true,
    "suppressImplicitAnyIndexErrors":true,
    "noUnusedLocals":true,
    "allowSyntheticDefaultImports":true
  },
  "include":["src/**/*"],
  "exclude":["node_modules"]
}